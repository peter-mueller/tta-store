{
  "entrypoint": "index.html",
  "shell": "src/asset-store.html",
  "fragments": [
    "src/view-unit.html",
    "src/view-404.html"
  ],
  "sources": [
    "images/**/*",
    "src/**/*",
    "data/**/*"
  ],
  "extraDependencies": [
    "bower_components/webcomponentsjs/*.js",
    "!bower_components/webcomponentsjs/gulpfile.js",
    "manifest.json"
  ],
  "lint": {
    "rules": ["polymer-2"]
  },
  "builds": [
    {
      "preset": "es5-bundled",
      "basePath": "/tta-store/"
    },
    {
      "preset": "es6-bundled"
    },
    {
      "preset": "es6-unbundled"
    }
  ]
}
